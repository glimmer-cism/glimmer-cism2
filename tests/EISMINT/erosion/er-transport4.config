# configuration for the EISMINT-2 test-case

[EISMINT-2]

[grid]
# grid sizes
ewn = 76
nsn = 76
upn = 11
dew = 20000
dns = 20000

[options]
topo_is_relaxed = 1
ttemperature = 1
flow_law = 0
isostasy = 0
marine_margin = 2
stress_calc = 2
evolution = 0
basal_water = 0
slip_coeff = 2
sliding_law = 0
vertical_integration = 1

[time]
tend = 200000.
dt = 5.
ntem = 1.
nvel = 1.
niso = 1.

[parameters]
flow_factor = 1
geothermal = -42e-3
basal_tract_const = 1.e-3

[Erosion]

[Basic_Transport]

[isostasy]
lithosphere = 1
asthenosphere = 1

[CF default]
title: transport, elra
comment: erosion + transport, with elra

[CF output]
name: er-transport4.nc
frequency: 1000
variables: thk bmlt bwat topg erate erosion seds1 seds2 seds3 taux tauy seds2_max seds2_vx seds2_vy slc relx

[CF input]
name: topo.nc
time: 1

