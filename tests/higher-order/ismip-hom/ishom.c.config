[ISMIP-HOM-TEST]
# Test C from ISMIP-HOM higher-order test suite
# see: Pattyn et al., The Cryosphere, 2, 95â€“108, 2008.
# www.the-cryosphere.net/2/95/2008

[parameters]
flow_factor = 1

[CF output]
variables = uvelhom vvelhom wvel tau_xz tau_yz
frequency = 1
name = ishom.c.out.nc

[grid]
upn = 11
ewn = 41
nsn = 41
dew = 3200
dns = 3200
sigma_builtin = 1

[time]
tstart = 0.
tend = 1.
dt = 1.

[CF default]
comment = generated by runGlimmer.py
title = ISMIP-HOM Experiment C

[options]
flow_law = 2            # constant and uniform rate factor
periodic_ew = 1         # doubly periodic lateral boundary conditions
periodic_ns = 1
diagnostic_run = 1      # combination of this and next flag give diag. soln. only
evolution = 0

[ho_options]
diagnostic_scheme = 3   # Payne/Price 1st-order dynamics
which_ho_babc = 5       # doubly periodic basal traction param. from .nc input file
which_ho_efvs = 0       # nonlinear eff. visc. w/ n=3
which_ho_sparse = 1     # use SLAP GMRES for linear solver

[CF input]
name = ishom.c.nc
time = 1
