[DOME-TEST]

[parameters]
flow_factor = 1
ice_limit = 0

[CF output]
variables = thk usurf uvelhom vvelhom velnormhom
frequency = 1
name = output/dome.out.nc

[grid]
upn = 10
ewn = 30
nsn = 30
dew = 2000
dns = 2000
sigma_builtin = 1

[time]
tend = 100.
tstart = 0.
dt = 1.

[CF default]
comment = created with dome.py 
title = parabolic dome test case using first-order dynamics  

[options]
flow_law = 2            # isothermal
diagnostic_run = 1      # combination of this and next flag give diag. soln. only 
evolution = 0       

[ho_options]
diagnostic_scheme = 3   # Payne/Price 1st-order dynamics
which_ho_babc = 4       # no-slip at bed
which_ho_efvs = 0       # nonlinear eff. visc. w/ n=3
which_ho_sparse = 1     # use SLAP GMRES for linear solver

[CF input]
name = output/dome.nc
time = 1
